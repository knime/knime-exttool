<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.knime.workbench.repository.nodes">
      <node
            category-path="/misc"
            factory-class="org.knime.ext.ssh.node.ExtSSHToolNodeFactory"
            id="org.knime.ext.ssh.node.ExtSSHToolNodeFactory"/>
   </extension>
  <extension
        point="org.eclipse.ecf.filetransfer.urlStreamHandlerService">
     <urlStreamHandlerService
           protocol="sftp"
           serviceClass="org.knime.ext.ssh.SftpURLStreamHandler">
     </urlStreamHandlerService>
  </extension>   
</plugin>
